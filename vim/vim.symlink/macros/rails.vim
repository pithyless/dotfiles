Rnavcommand config config -glob=**/* -default=routes.rb -suffix=
Rnavcommand test test                    -glob=**/* -default=test_helper -suffix=.rb
Rnavcommand sass app/stylesheets/ -glob=**/* -default=application -suffix=.sass
Rnavcommand scss app/stylesheets/ -glob=**/* -default=application -suffix=.scss
Rnavcommand db db -glob=**/* -default=schema.rb -suffix=
Rnavcommand feature features -glob=**/* -suffix=.feature
Rnavcommand step features/step_definitions -glob=* -suffix=.rb

